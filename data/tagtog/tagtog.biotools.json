                    },
                    {
                        "name": "Stefancsik R."
                    },
                    {
                        "name": "Millburn G.H."
                    },
                    {
                        "name": "Rost B."
                    }
                ],
                "citationCount": 12,
                "date": "2014-01-01T00:00:00Z",
                "journal": "Database",
                "title": "Tagtog: Interactive and text-mining-assisted annotation of gene mentions in PLOS full-text articles"
            },
            "type": [
                "Primary"
            ]
        }
