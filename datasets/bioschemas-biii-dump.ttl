        <http://edamontology.org/operation_Cell_segmentation>,
        <http://edamontology.org/operation__RyCFuPLOeGxpcGjYbWcWWD> ;
    ns1:name "Cell or particle counting and scoring the percentage of stained objects" ;
    ns1:publisher "Cell Profiler Team" ;
    ns1:softwareRequirements <http://biii.eu/node/31> .

